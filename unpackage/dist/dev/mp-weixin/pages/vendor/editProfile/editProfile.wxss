}
.userImage {
		bottom: 24%;
		left: 10%;
		width: 130px;
		height: 130px;
		overflow: hidden;
		border-radius: 50%;
		border: 2px solid white;
image {
			width: 100%;
}
}
.text {
		margin-top: 13px;
		color: #000000;
